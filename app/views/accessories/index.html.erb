
<% @bike_accessories.each do | accessory| %>
	Does this bike have a <strong><%= accessory.name %>?</strong>
<%= image_tag accessory.image.url(:medium) %>

		<%= paginate @bike_accessories %>

		<%= form_for(accessory) do |f| %>
			<% f.hidden_field :not_missing, value: false %>
			<%= f.submit "No" %>
		<% end %>
<% end %>

